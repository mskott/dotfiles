[user]
	name = Martin Sk√∏tt
	email = martin@skoett.name

[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
	autocrlf = input
	excludesfile = /home/martin/.gitexcludes
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
#[credential]
#	helper = libsecret
[credential]
	helper = cache --timeout 7200
	helper = oauth
